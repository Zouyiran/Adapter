$def with(TransactionID, TimeStamp, devList)

<?xml version="1.0" encoding="UTF-8" ?>
<ServiceResponse>
    <TransactionID>$TransactionID</TransactionID>
    <TimeStamp>$TimeStamp</TimeStamp>
    $for device in devList:
        <DevIns>
            <InsID>$device["InsID"]</InsID>
            <Status>$device["Status"]</Status>
            <Desc>$device["Desc"]</Desc>
            <CurTime>$device["CurTime"]</CurTime>
        </DevIns>
</ServiceResponse>